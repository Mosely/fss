{
	"name" : "fss/fss",
	"description" : "The RESTful API for the Family Support Services system.",
	"keywords" : [
		"rest"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Dewayne Higgs",
			"email" : "dewayne.higgs@gmail.com",
			"role" : "Author"
		}
	],
	"require" : {
		"php" : ">=5.5.0",
		"slim/slim" : "^3.1",
		"monolog/monolog" : "^1.17",
		"illuminate/database" : "5.*",
		"tuupola/slim-jwt-auth" : "2.*",
		"vlucas/phpdotenv" : "2.*",
		"phpstan/phpstan" : "~0.8"
	},
	"require-dev" : {
		"phpunit/phpunit" : ">=4.8 < 6.0"
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\" : "tests/",
			"FSS\\" : "src/",
			"FSS\\Models\\" : "src/Models/",
			"FSS\\Controllers\\" : "src/Controllers/",
			"FSS\\Utilities\\" : "src/Utilities/"
		}
	},
	"scripts" : {
		"start" : "php -S 0.0.0.0:8080 -t public public/index.php",
		"test" : "phpunit"
	},
	"minimum-stability" : "alpha",
	"autoload" : {
		"psr-4" : {
			"FSS\\" : "src/",
			"FSS\\Models\\" : "src/Models/",
			"FSS\\Controllers\\" : "src/Controllers/",
			"FSS\\Utilities\\" : "src/Utilities/"
		}
	}
}