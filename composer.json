{
	"name" : "fss/fss",
	"description" : "The RESTful API for the Family Support Services system.",
	"keywords" : [
		"rest"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Dewayne Higgs",
			"role" : "Author"
		}, {
			"name" : "BJ Brooks",
			"role" : "Author"
		}, {
			"name" : "Marshal Homfeld",
			"role" : "Author"
		}
	],
	"require" : {
		"php" : ">=7",
		"slim/slim" : "^3",
		"monolog/monolog" : "^1",
		"illuminate/database" : "^5",
		"tuupola/slim-jwt-auth" : "^2",
		"vlucas/phpdotenv" : "^2",
		"phpoffice/phpspreadsheet" : "dev-master",
		"phpunit/phpunit" : "~6.3",
		"thiagoalessio/tesseract_ocr" : "^1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~6.3",
		"php" : ">=7",
		"slim/slim" : "^3",
		"monolog/monolog" : "^1",
		"illuminate/database" : "^5",
		"tuupola/slim-jwt-auth" : "^2",
		"vlucas/phpdotenv" : "^2",
		"phpoffice/phpspreadsheet" : "dev-master",
		"thiagoalessio/tesseract_ocr" : "^1"
	},
	"autoload-dev" : {
		"psr-4" : {
			"Tests\\Functional\\" : "tests/Functional/",
			"FSS\\" : "src/",
			"FSS\\Models\\" : "src/Models/",
			"FSS\\Controllers\\" : "src/Controllers/",
			"FSS\\Utilities\\" : "src/Utilities/"
		}
	},
	"scripts" : {
		"start" : "php -S 0.0.0.0:8080 -t public public/index.php",
		"test" : "phpunit"
	},
	"minimum-stability" : "dev",
	"autoload" : {
		"psr-4" : {
			"Tests\\" : "tests/",
			"FSS\\" : "src/",
			"FSS\\Models\\" : "src/Models/",
			"FSS\\Controllers\\" : "src/Controllers/",
			"FSS\\Utilities\\" : "src/Utilities/"
		}
	}
}